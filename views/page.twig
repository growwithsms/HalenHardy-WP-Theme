{% extends "base.twig" %}

{% block content %}

	<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
		<section>
			<div class="article-header wrapper">
				<h1>{{post.title}}</h1>
			</div>
			<div class="article-body-wrapper">
				<div class="article-body wrapper">
					{{post.content}}
				</div>
			</div>
		</section>
	</article>
{% endblock %}