{% extends "base.twig" %}

{% block content %}
	<div id="oil"></div>
	{% if post.get_field('hero_content') %}
		<section class="hero">
			<div class="hero-top">
				<div class="wrapper">
					<h1>{{post.get_field('hero_header')}}</h1>
					<a href="{{post.get_field('hero_button_url')}}" class="button orange">{{post.get_field('hero_button_text')}}</a>
				</div>
			</div>
			<div class="hero-bottom">
				<div class="wrapper">
					<h2>Are you a...</h2>
					<div class="industries" id="industries">
						{% for industry in post.get_field('industries') %}
					        <a href="{{industry.url}}" class="industry">
					            <h3>{{industry.name}}</h3>
					        </a>
					    {% endfor %}
					</div>
				</div>
			</div>
		</section>
	{% endif %}
	<section class="blocky-grid">
		{% for grid_block in post.get_field('pages_grid') %}
			{% if grid_block.acf_fc_layout == 'medium_video_block' %}
				<div class="blocky-grid-item medium video">
					<div class="blocky-grid-item--inner">
						{{grid_block.video_embed}}
					</div>
				</div>
			{% endif %}
			{% if grid_block.acf_fc_layout == 'medium_text_block' %}
				<div class="blocky-grid-item medium text">
					<div class="blocky-grid-item--inner">
						<h3>{{grid_block.header}}</h3>
						<p>{{grid_block.description}}</p>
						<p class="text-right"><a href="{{grid_block.button_url}}" class="button white">{{grid_block.button_text}}</a>
					</div>
				</div>
			{% endif %}
			{% if grid_block.acf_fc_layout == 'medium_blog_block' %}
				<div class="blocky-grid-item small blog post">
					<div class="blocky-grid-item--inner">
						{{grid_block.rss_aggregator_shortcode}}
					</div>
				</div>
				<div class="blocky-grid-item small blog listing">
					<div class="blocky-grid-item--inner">
						<h3>{{grid_block.header}}</h3>
						<div class="blocky-grid-item--inner_hover">
